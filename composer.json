{
    "name": "rebelcode/migrations",
    "description": "Simple database migration library.",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.4 | ^7.0",
        "byjg/migration": "dev-master as 2.1",
        "dhii/exception-interface": "^0.1 | ^0.2",
        "dhii/exception": "0.1-alpha4",
        "dhii/i18n-helper-base": "0.1-alpha1",
        "dhii/normalization-helper-base": "0.1-alpha1 | 0.1-alpha2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "phpunit/dbunit": "2.0.3",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "mikey179/vfsStream": "^1.6.5",
        "dhii/stringable-interface": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\Migrations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RebelCode\\Migrations\\TestStub\\": "test/stub"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/RebelCode/migration"
        },
        {
            "type": "vcs",
            "url": "http://github.com/RebelCode/anydataset"
        }
    ]
}
